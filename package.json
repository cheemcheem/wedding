{
  "name": "wedding",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/**"
  ],
  "modular": {
    "type": "root"
  },
  "scripts": {
    "start": "BROWSER=none modular start @wedding/app",
    "build": "modular build @wedding/app",
    "test": "modular test",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "postinstall": "patch-package",
    "prettier": "prettier --write ."
  },
  "eslintConfig": {
    "extends": "modular-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 80,
    "proseWrap": "always"
  },
  "dependencies": {
    "@jpmorganchase/uitk-core": "^0.7.0",
    "@jpmorganchase/uitk-icons": "^0.6.0",
    "@jpmorganchase/uitk-lab": "^0.7.1",
    "@jpmorganchase/uitk-theme": "^0.5.0",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "eslint-config-modular-app": "^3.0.2",
    "ics": "^2.41.0",
    "js-file-download": "^0.4.12",
    "modular-scripts": "^3.6.0",
    "modular-template-app": "^1.1.0",
    "modular-template-package": "^1.1.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": ">=4.2.1 <4.5.0"
  },
  "devDependencies": {
    "patch-package": "^6.5.0",
    "raw-loader": "^4.0.2"
  }
}
